{
  "name": "bughouse",
  "version": "0.1.0",
  "author": "Sonny Michaud <michaud.sonny@gmail.com> (https://github.com/sonnym)",
  "repository": {
    "type": "git",
    "url": "git://github.com/sonnym/bughouse.git"
  },
  "scripts": {
    "build": "./node_modules/.bin/webpack --config config/webpack.config.js",
    "start": "./node_modules/.bin/pm2 start config/pm2.development.config.js",
    "stop": "./node_modules/.bin/pm2 kill",
    "restart": "./node_modules/.bin/pm2 restart all",
    "test": "yarn test:client; yarn test:server; yarn test:simul; ./node_modules/.bin/nyc report",
    "test:client": "./node_modules/.bin/nyc ./node_modules/.bin/ava --config ava.config.browser.js",
    "test:server": "./node_modules/.bin/nyc --no-clean ./node_modules/.bin/ava --config ava.config.server.js",
    "test:simul": "./node_modules/.bin/nyc --no-clean ./node_modules/.bin/ava --config ava.config.simul.js",
    "flushdb": "redis-cli -n 1 flushdb",
    "lint": "./node_modules/.bin/eslint src",
    "migrate": "./node_modules/.bin/knex --knexfile=config/knex.config.js migrate:latest",
    "migrate:down": "./node_modules/.bin/knex --knexfile=config/knex.config.js migrate:down",
    "migrate:make": "./node_modules/.bin/knex --knexfile=config/knex.config.js migrate:make"
  },
  "dependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "apollo-server-express": "^2.12.0",
    "bcrypt": "^3.0.0",
    "body-parser": "^1.18.3",
    "bookshelf": "^1.1.1",
    "chess.js": "https://github.com/jhlywa/chess.js.git#master",
    "connect-redis": "^4.0.3",
    "express": "^4.15.4",
    "express-session": "^1.15.6",
    "express-ws": "^4.0.0",
    "knex": "^0.20.15",
    "nodemailer": "^6.3.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.2.0",
    "redis": "^3.0.2",
    "uuid": "^3.1.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@apollo/client": "^3.0.0-beta.43",
    "@ava/babel": "^1.0.1",
    "@babel/core": "^7.0.0",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "@babel/runtime": "^7.2.0",
    "@vue/test-utils": "^1.0.0-beta.33",
    "ava": "^3.7.1",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-add-filehash": "^6.9.4",
    "babel-plugin-transform-imports": "^2.0.0",
    "babel-plugin-webpack-alias-7": "^0.1.1",
    "browser-env": "^3.2.0",
    "css-loader": "^3.2.0",
    "eslint": "^6.5.1",
    "eslint-plugin-vue": "^6.0.1",
    "factory-girl": "*",
    "factory-girl-bookshelf": "^1.0.3",
    "imports-loader": "^0.8.0",
    "mini-css-extract-plugin": "^0.9.0",
    "nock": "^11.7.0",
    "nyc": "^15.0.0",
    "pm2": "^4.1.2",
    "ramda": "^0.26.1",
    "require-extension-hooks": "^0.3.0",
    "require-extension-hooks-babel": "^1.0.0-beta.1",
    "require-extension-hooks-vue": "^3.0.0",
    "sass": "^1.23.0",
    "sass-loader": "^8.0.0",
    "sinon": "^9.0.2",
    "supertest": "^4.0.2",
    "vue": "^2.5.13",
    "vue-cli": "^2.8.2",
    "vue-language-server": "0.0.65",
    "vue-loader": "^15.4.2",
    "vue-router": "^3.1.6",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.0.0",
    "vuetify": "^2.2.22",
    "vuetify-loader": "^1.1.0",
    "vuex": "^3.1.3",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2",
    "ws": "^7.2.0"
  },
  "license": "MIT",
  "nyc": {
    "all": true,
    "include": [
      "src"
    ],
    "reporter": [
      "html"
    ],
    "extension": [
      ".js",
      ".vue"
    ]
  }
}
