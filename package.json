{
  "name": "bughouse",
  "version": "0.1.0",
  "author": "Sonny Michaud <michaud.sonny@gmail.com> (https://github.com/sonnym)",
  "repository": {
    "type": "git",
    "url": "git://github.com/sonnym/bughouse.git"
  },
  "scripts": {
    "build": "./node_modules/.bin/webpack --config config/webpack.config.js",
    "lint": "./node_modules/.bin/eslint src",
    "simulate": "./node_modules/.bin/pm2 start config/simulation.pm2.config.js",
    "start": "./node_modules/.bin/pm2 start config/development.pm2.config.js",
    "stop": "./node_modules/.bin/pm2 kill",
    "test": "NODE_ENV=test ./node_modules/.bin/nyc ./node_modules/.bin/ava"
  },
  "dependencies": {
    "alekhine": "^0.8.0-alpha.4",
    "babel-cli": "^6.24.1",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "bookshelf": "^0.12.0",
    "bunyan": "^1.8.12",
    "express": "^4.15.4",
    "express-ws": "^3.0.0",
    "knex": "^0.14.2",
    "nodemailer": "^4.4.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.2.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-filehash": "^6.9.4",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "browser-env": "^3.2.0",
    "css-loader": "^0.28.5",
    "eslint": "^4.14.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "factory-girl-bookshelf": "^1.0.3",
    "node-sass": "^4.5.3",
    "nyc": "^11.1.0",
    "pm2": "^2.6.1",
    "ramda": "^0.25.0",
    "require-extension-hooks": "^0.3.0",
    "require-extension-hooks-babel": "^0.1.1",
    "require-extension-hooks-vue": "^0.4.0",
    "sass-loader": "^6.0.6",
    "sinon": "^4.1.3",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "supertest": "^3.0.0",
    "vue": "^2.5.13",
    "vue-cli": "^2.8.2",
    "vue-loader": "^14.0.4",
    "vue-router": "^3.0.1",
    "vuetify": "^0.17.6",
    "webpack": "^3.5.5",
    "ws": "^4.0.0"
  },
  "license": "MIT",
  "ava": {
    "require": [
      "babel-register",
      "./test/helpers/setup.js"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "extension": [
      ".js",
      ".vue"
    ]
  }
}
