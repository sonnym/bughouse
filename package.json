    "pg": "^7.2.0",
    "sequelize": "^4.8.0",
    "sequelize-cli": "^2.8.0",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-rewire": "^1.1.0",
